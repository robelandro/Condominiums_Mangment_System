<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="350.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="login.LoginController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="DB Statues" textFill="#23b276">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="dbStatues" layoutX="85.0" layoutY="14.0" />
      <Label layoutX="265.0" layoutY="48.0" text="Login" textFill="#9e5959">
         <font>
            <Font name="System Bold Italic" size="16.0" />
         </font>
      </Label>
      <Label layoutX="85.0" layoutY="150.0" text="UserName" />
      <Label layoutX="85.0" layoutY="200.0" text="Password" />
      <TextField fx:id="userName" layoutX="190.0" layoutY="147.0" prefHeight="26.0" prefWidth="248.0" />
      <PasswordField fx:id="password" layoutX="190.0" layoutY="197.0" prefHeight="26.0" prefWidth="248.0" />
      <ComboBox fx:id="option" layoutX="190.0" layoutY="98.0" prefWidth="150.0" promptText="Admin/Staff" />
      <Label layoutX="96.0" layoutY="102.0" text="Choose" />
      <Button fx:id="login" layoutX="205.0" layoutY="245.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="120.0" text="Log In" />
      <Button fx:id="forgetPassword" layoutX="191.0" layoutY="283.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="248.0" text="Forgot password?" />
      <Label fx:id="errorPrint" layoutX="82.0" layoutY="249.0" prefHeight="17.0" prefWidth="56.0" />
   </children>
</AnchorPane>
